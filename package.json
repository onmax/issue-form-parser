{
  "name": "issue-form-parser",
  "version": "20",
  "description": "A GitHub Action that parses issues created by Issue Forms to JSON format",
  "main": "lib/main.js",
  "scripts": {
    "build": "npm run package",
    "ci-test": "npx jest",
    "package": "tsc",
    "test": "npx jest",
    "all": "npm run test && npm run package",
    "release": "npm run build && bumpp --commit --push --tag && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "github",
    "issue form",
    "parser"
  ],
  "author": "onmax",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/node": "^18.19.71",
    "@vercel/ncc": "^0.38.3",
    "bumpp": "^9.10.1",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  }
}