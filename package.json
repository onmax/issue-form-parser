{
  "name": "issue-form-parser",
  "version": "1.4",
  "description": "A GitHub Action that parses issues created by Issue Forms to JSON format",
  "main": "lib/main.js",
  "scripts": {
    "build": "npm run package",
    "ci-test": "npx jest",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "package": "tsc",
    "test": "npx jest",
    "all": "npm run test && npm run coverage && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "github",
    "issue form",
    "parser"
  ],
  "author": "onmax",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "axios": "^1.2.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@vercel/ncc": "^0.38.1",
    "jest": "^29.1.2",
    "js-yaml": "^4.1.0",
    "ts-jest": "^29.1.2",
    "typescript": "^4.4.4"
  }
}
